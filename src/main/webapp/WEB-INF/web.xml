<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>ItinerantApp</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.jsp</welcome-file>
    <welcome-file>default.htm</welcome-file>
  </welcome-file-list>
  
  <servlet>
  	<servlet-name>ListaProfesionalesServlet</servlet-name>
  	<servlet-class>com.itinerant.controller.admin.ListaProfesionalesServlet</servlet-class>
  </servlet> 
  <servlet-mapping>
  	<servlet-name>ListaProfesionalesServlet</servlet-name>
  	<url-pattern>/admin/lista_profesionales</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>ListaSupervisoresServlet</servlet-name>
  	<servlet-class>com.itinerant.controller.admin.ListaSupervisoresServlet</servlet-class>
  </servlet>  
  <servlet-mapping>
  	<servlet-name>ListaSupervisoresServlet</servlet-name>
  	<url-pattern>/admin/lista_supervisores</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>ListaCertificadosServlet</servlet-name>
  	<servlet-class>com.itinerant.controller.admin.ListaCertificadosServlet</servlet-class>
  </servlet>  
  <servlet-mapping>
  	<servlet-name>ListaCertificadosServlet</servlet-name>
  	<url-pattern>/admin/lista_certificados</url-pattern>
  </servlet-mapping>
</web-app>